<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="422.0" prefWidth="419.0" xmlns="http://javafx.com/javafx/20.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="its.biblioteca.bibliotecafx.application.AddUserController">
   <Label layoutX="18.0" layoutY="33.0" prefHeight="17.0" prefWidth="383.0" text="Name"/>
   <TextField fx:id="nameField" layoutX="18.0" layoutY="50.0" prefHeight="25.0" prefWidth="383.0"/>
   <Label layoutX="18.0" layoutY="81.0" prefHeight="17.0" prefWidth="383.0" text="Surname"/>
   <TextField fx:id="surnameField" layoutX="18.0" layoutY="98.0" prefHeight="25.0" prefWidth="383.0"/>
   <Label layoutX="18.0" layoutY="130.0" prefHeight="17.0" prefWidth="383.0" text="E-mail"/>
   <TextField fx:id="emailField" layoutX="18.0" layoutY="147.0" prefHeight="25.0" prefWidth="383.0"/>
   <Label layoutX="18.0" layoutY="182.0" prefHeight="17.0" prefWidth="383.0"
          text="Phone number"/>
   <TextField fx:id="phoneNumberField" layoutX="18.0" layoutY="199.0" prefHeight="25.0" prefWidth="383.0"/>
   <Label layoutX="18.0" layoutY="288.0" prefHeight="17.0" prefWidth="383.0" text="Address"/>
   <TextField fx:id="addressField" layoutX="18.0" layoutY="305.0" prefHeight="25.0" prefWidth="383.0"/>
   <Label layoutX="18.0" layoutY="236.0" prefHeight="17.0" prefWidth="383.0"
          text="Date of Birth"/>
   <DatePicker fx:id="dateOfBirthPicker" layoutX="18.0" layoutY="253.0" prefHeight="25.0" prefWidth="383.0"/>
   <Button fx:id="backButton" layoutX="360.0" layoutY="383.0" mnemonicParsing="false" text="Back"/>
   <Button fx:id="saveButton" layoutX="309.0" layoutY="383.0" mnemonicParsing="false" onAction="#handleSaveAction" text="Save"/>
</AnchorPane>
